{
    "name": "skaparate/testmodule",
    "description": "Showcases how to override a Symfony controller route",
    "type": "prestashop-module",
    "authors": [
        {
            "name": "Skaparate",
            "email": "nico@nicomv.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^v2.18.2",
        "prestashop/php-dev-tools": "^3.12",
        "phpunit/phpunit": "8.5"
    },
    "scripts": {
        "php-cs-fixer": "php-cs-fixer",
        "format": "php-cs-fixer fix"
    },
    "autoload": {
        "psr-4": {
            "Skaparate\\": "src/"
        },
        "classmap": [
            "sk_testmodule.php"
        ],
        "exclude-from-classmap": []
    },
    "config": {
        "preferred-install": "dist",
        "prepend-autoloader": false
    }
}
